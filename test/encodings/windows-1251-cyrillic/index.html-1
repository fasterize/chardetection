                    var wrap = $('.superscreen_wrap');

                    if (inner.height() < 250) {
                        wrap.css('height', 250);
                    }
                }

                function onSuperbannerFailed() {
                    $("#hide_adv, #top_superscreen").hide();
                    $("#top_3banners").show();
