                                    function daydiff(first, second) {
									    return Math.round((second-first)/(1000*60*60*24));
								    }
                                    function addzero(txt)
                                    {
                                        if(txt.toString().length==1)
                                            txt = "0" + txt;
                                        return txt;
                                    }
                                

                                    addVakit();

                                    function msToTime(s) {
                                      var ms = s % 1000;
                                      s = (s - ms) / 1000;
                                      var secs = s % 60;
                                      s = (s - secs) / 60;
                                      var mins = s % 60;
                                      var hrs = (s - mins) / 60;

                                      return addzero(hrs) + ":" + addzero(mins) + ":" + addzero(secs) ;
                                    }
                                    
