                  kalanSaniye = kalanSaniye > 9 ? kalanSaniye : "0" + kalanSaniye;

                  sayacDiv.html(+saat + ':' + dakika + ':' + kalanSaniye);
              }
              setInterval('$.azalt()', 1000);


          });
      }

  })(jQuery);


