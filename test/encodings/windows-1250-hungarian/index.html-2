<script>

var rodo_cookies_nbr_of_switches = 2;
var rodo_cookies_reload = false;

function set_rodo_cookies(val) {
	$.ajax({
		url : '/ajax/rodo.asp',
		type : 'GET',
		data : {'rodo': val},
   	success : function(data) {
   	}
	})
	rodo_cookies_reload = true;
}

function close_rodo_cookies_panel(obj) {
	$('.cookie-alert-2018-c').addClass('cookie-alert-2018-c-hide');
	send_rodo_event(obj);
	if (rodo_cookies_reload) {
		setTimeout(function(){location.reload();}, 500);
	};
}
$('[id^=rodo-cookies-close-]').bind('click',function(){close_rodo_cookies_panel(this)});

function show_rodo_cookies_settings(obj) {
	$('.cookie-alert-2018-1').addClass('cookie-alert-2018-1-hide');
	$('.cookie-alert-2018-2-c').addClass('cookie-alert-2018-2-c-show');
	send_rodo_event(obj);
}
$('#rodo-cookies-settings').bind('click',function(){show_rodo_cookies_settings(this)});

function toggle_rodo_cookies_switch(obj) {
	var rodo_cookies_switch = ($(obj).attr('id')).replace('rodo-cookies-switch-','');
	$('#rodo-cookies-switch-' + rodo_cookies_switch).toggleClass('cook18-but-sw-off');
	var val = '2';
	for (var i = 1;i <= rodo_cookies_nbr_of_switches;i++) {
		if (!$('#rodo-cookies-switch-' + i).hasClass('cook18-but-sw-off')) {
			val = val + '1';
		} else {
			val = val + '0';
		};
	};
	set_rodo_cookies(val);
	send_rodo_event(obj);
	$('#rodo-cookies-all-2').hide();
	$('#rodo-cookies-close-3').show();
}
$('[id^=rodo-cookies-switch-]').bind('click',function(){toggle_rodo_cookies_switch(this)});

function accept_all_rodo_cookies(obj) {
	set_rodo_cookies(1);
	close_rodo_cookies_panel(obj);
}
$('[id^=rodo-cookies-all-]').bind('click',function(){accept_all_rodo_cookies(this)});

function send_rodo_event(obj) {
	ga('send', 'event', 'rodopopup', 'click', $(obj).attr('id'));
};

</script>
