        for (var i = 0, len = ranges.length; i < len; ++i) {
            min = ranges[i][0], max = ranges[i][1];
            if (min_value < max && max_value >= min) {
                final_ranges.push(min + '-' + (max !== Number.POSITIVE_INFINITY ? max : 'mais'));
